@using MRTD.Core.Common;
@using BusinessSchoolMLS.SchoolBusinessComponent;


@{
    ViewData["Title"] = "Home";
    bool IsStudent = false;
    if (Session.AppSession.ContainsKey("MemberID"))
    {
        LoginBusinessComponent loginBusinessComponent = new LoginBusinessComponent();
        IsStudent = loginBusinessComponent.IsStudentCheckByMemberGuid(Session.AppSession["MemberID"].ToString());
    }
}
@if (!IsStudent)
{
    @Html.Partial("BusinessSchoolDashBoard")
}
else
{
    @Html.Partial("StudentDashBoard")
}